extends layout

block content
    wh-map#map-canvas(
            center="map.center",
            markers="map.markers",
            places="map.places",
            events="events"
        )
        wh-marker(markers="map.markers")
        wh-marker(markers="map.places")
    section.directions
        form
            h1 Meet Me Halfway
            hr
            label
                h2 Address 1
                input.long(type='text')
            label
                h2 Address 2
                input.long(type='text')
            button.button-submit Submit
    script(type='text/javascript', src='javascripts/app.min.js')
